                                selKeyword.defaultVal = selKeyword.value;
                                var autoclpT2 = '';
                                var keyworldchar = '';
                                var cache = new Array();
                                var objt = {};
                                var loaded = false;
                                var loadedli = {};
                                var linow = 0;
                                var liLen = 0;
