                                            url = encodeURI(url);
                                            url = encodeURI(url);
                                            window.open(url);
                                            return false;
                                        });
                                var iniHtml = function() {
                                    clearTimeout(autoclpT2);
                                    var data = cache["'" + keyworldchar + "'"];
                                    var len = data.length;
